{
  "name": "canaan/web-socket",
  "description": "Laravel library for asynchronously serving WebSockets. ",
  "keywords": [
    "framework",
    "laravel",
    "web",
    "websocket",
    "socket",
    "php",
    "server"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Canaan Etai",
      "email": "me@canaane.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "cboden/ratchet": "^0.4"
  },
  "autoload": {
    "psr-4": {
      "Canaane\\Socket\\": "src/"
    }
  },
  "extra": {
      "laravel": {
          "providers": [
              "Canaane\\Socket\\Providers\\SocketServiceProvider"
          ]
      }
  },
  "prefer-stable": true
}
